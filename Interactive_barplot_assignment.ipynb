   "source": [
    "%matplotlib notebook\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "from matplotlib.colors import Normalize\n",
    "from matplotlib.cm import get_cmap\n",
    "\n",
    "# Calculating the mean of each sample\n",
    "df_mean = df.mean(axis=1)\n",
    "\n",
    "# Calculating confidence interval 95% with STD (without mean)\n",
    "c = 1.960\n",
    "df_ci = (c * df.std(axis=1) / np.sqrt(df.shape[1]))\n",
    "\n",
    "# Set y value\n",
    "y = 37000\n",
    "\n",
    "# Create colormap -- from dark blue to white / from white to dark red => seismic\n",
    "color_scaler = Normalize(vmin= -c, vmax= c)\n",
    "color_map = get_cmap('seismic', 11)\n",
    "\n",
    "colors_df = pd.DataFrame()\n",
    "\n",
    "colors_df['ColorIntensity'] = color_scaler((df_mean - y) / df_ci)\n",
    "colors_df['Colors'] = [color_map(x) for x in colors_df['ColorIntensity']]\n",
    "\n",
    "# Data Visialuzation bar chart\n",
    "plt.figure(figsize=(9, 7))\n",
    "bar_chart = plt.bar(df.index, df_mean, yerr= df_ci, capsize=15, color=colors_df['Colors'], edgecolor= 'black')\n",
    "y_line = plt.axhline(y= y, color='k', linewidth= 1, linestyle= '--')\n",
    "y_interest = plt.text(1995.68, y, 'y= %d'%y, bbox=dict(facecolor= 'white', edgecolor= 'black'))\n",
    "\n",
    "# Add xticks as years \n",
    "plt.xticks(df.index)\n",
    "\n",
    "# Add axes labels and plot title\n",
    "plt.gca().set_ylabel('Votes in County')\n",
    "plt.title('Certainty To Be In Distribution For {} '.format(np.around(y, decimals= 2)))\n",
    "\n",
    "# Add colorbar -- bluer and bluer : distribution < y -- white : value contained in distribution \n",
    "# -- redder and redder : value not in distribution > y\n",
    "# Need to create ScalarMappable in order to use colobar\n",
    "sm = plt.cm.ScalarMappable(cmap=color_map, norm=Normalize(vmin=0, vmax=1))\n",
    "sm.set_array([])\n",
    "\n",
    "color_bar = plt.colorbar(sm, ticks= np.around(np.linspace(0, 1, 12), decimals=2), orientation='horizontal',\n",
    "                        pad = 0.12, shrink = 0.85)\n",
    "color_bar.set_label('Zero means “certainly below” -- One means “certainly above”', labelpad= 12)\n",
    "\n",
    "# Let user input y value by clicking on the plot\n",
    "def onclick(event):\n",
    "    for i in range(4):\n",
    "        indiv_colors = color_map(color_scaler((df_mean.values[i] - event.ydata)/df_ci.values[i]))\n",
    "        bar_chart[i].set_color(indiv_colors)\n",
    "        bar_chart[i].set_edgecolor(\"black\")\n",
    "        \n",
    "    y_line.set_ydata(event.ydata)\n",
    "    y_interest.set_text('y= %d' %event.ydata)\n",
    "    y_interest.set_position((1995.68, event.ydata))\n",
    "    \n",
    "    plt.gca().set_title('Certainty To Be In Distribution For {} '.format(np.around(event.ydata, decimals= 2)))\n",
    "\n",
    "# tell mpl_connect we want to pass a 'button_press_event' into onclick when the event is detected\n",
    "plt.gcf().canvas.mpl_connect('button_press_event', onclick)\n",
    "\n",
    "plt.show()"
